{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "outDir": "./build",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true   ,
    "resolveJsonModule": true 
  }
}
